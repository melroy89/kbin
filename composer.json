{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/api-platform/activity-pub"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "^2.6",
        "api-platform/activity-pub": "*",
        "babdev/pagerfanta-bundle": "^2.8",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/mongodb-odm-bundle": "^4.3",
        "doctrine/orm": "^2.8",
        "easycorp/easyadmin-bundle": "^3",
        "embed/embed": "dev-master",
        "friendsofsymfony/jsrouting-bundle": "^2.7",
        "karser/karser-recaptcha3-bundle": "^0.1.17",
        "knplabs/knp-time-bundle": "^1.15",
        "laminas/laminas-diactoros": "^2.5",
        "landrok/activitypub": "^0.5.6",
        "league/commonmark": "^1.5",
        "liip/imagine-bundle": "^2.4",
        "nelmio/cors-bundle": "^2.1",
        "nucleos/antispam-bundle": "^2.1",
        "oneup/flysystem-bundle": "^3.7",
        "pagerfanta/pagerfanta": "^2.5",
        "phpdocumentor/reflection-docblock": "^5.2",
        "sensio/framework-extra-bundle": "^5.6",
        "slevomat/coding-standard": "^6.4",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/asset": "5.2.*",
        "symfony/cache": "5.2.*",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/expression-language": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.2.*",
        "symfony/framework-bundle": "5.2.*",
        "symfony/http-client": "5.2.*",
        "symfony/mailer": "5.2.*",
        "symfony/mailgun-mailer": "5.2.*",
        "symfony/mercure-bundle": "^0.2.6",
        "symfony/messenger": "5.2.*",
        "symfony/mime": "5.2.*",
        "symfony/monolog-bundle": "^3.6",
        "symfony/property-access": "5.2.*",
        "symfony/property-info": "5.2.*",
        "symfony/proxy-manager-bridge": "5.2.*",
        "symfony/rate-limiter": "5.2.*",
        "symfony/security-bundle": "5.2.*",
        "symfony/serializer": "5.2.*",
        "symfony/twig-bundle": "5.2.*",
        "symfony/ux-chartjs": "^1.2",
        "symfony/validator": "5.2.*",
        "symfony/webpack-encore-bundle": "^1.8",
        "symfony/workflow": "5.2.*",
        "symfony/yaml": "5.2.*",
        "symfonycasts/verify-email-bundle": "^1.2",
        "tchoulom/view-counter-bundle": "^5.1",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/intl-extra": "^3.2",
        "twig/twig": "^2.12|^3.0",
        "webmozart/assert": "^1.9"
    },
    "require-dev": {
        "api-platform/schema-generator": "^3.0",
        "dama/doctrine-test-bundle": "^6.5",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "fzaninotto/faker": "^1.9",
        "symfony/browser-kit": "5.2.*",
        "symfony/css-selector": "5.2.*",
        "symfony/maker-bundle": "^1.27",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/stopwatch": "^5.2",
        "symfony/web-profiler-bundle": "^5.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    }
}
